{
  "manifest_version": 2,

  "name": "Site Load Plug-in",
  "description": "This extension will load certain pages of your choice.",
  "version": "1.0",

  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  "permissions": [
   "activeTab",
   "tabs",
   "http://*/*",
   "https://*/*"
   ]
}